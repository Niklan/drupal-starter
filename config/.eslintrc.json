{
  "extends": "./../web/core/.eslintrc.json",
  "root": true,
  "ignorePatterns": [
    "assets/vendor/**/*",
    "config/**/*",
    "var/**/*",
    "web/**/*",
    "vendor/**/*"
  ],
  "rules": {
    "prettier/prettier": "off",
    "valid-jsdoc": "off",
    "no-console": ["error", {"allow": ["warn", "error"]}]
  },
  "parserOptions": {
    "ecmaVersion": 2022
  }
}
